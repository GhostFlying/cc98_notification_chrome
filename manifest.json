{
  "manifest_version": 2,
  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "permissions": [
    "cookies",
    "*://*.cc98.org/*",
    "alarms",
    "notifications",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "page": "eventPages.html"
  }
}